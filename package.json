{
  "name": "tagdafun",
  "version": "1.1.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "ts:check": "tsc --noEmit",
    "build:android": "eas build --platform android --profile production",
    "build:android-preview": "eas build --platform android --profile preview",
    "build:android-dev": "eas build --platform android --profile development",
    "build:ios": "eas build --platform ios --profile production",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "build:script": "powershell -ExecutionPolicy Bypass -File build-android.ps1",
    "deploy:android": "npm run build:android && npm run submit:android",
    "deploy": "eas build --platform android --profile production"
  },
  "dependencies": {
    "expo": "~52.0.0",
    "expo-status-bar": "~2.0.0",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "@react-navigation/native": "^6.1.16",
    "@react-navigation/bottom-tabs": "^6.5.16",
    "react-native-screens": "~4.4.0",
    "react-native-safe-area-context": "4.12.0",
    "expo-font": "~13.0.4",
    "expo-constants": "~17.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.3.12",
    "typescript": "^5.3.0",
    "babel-plugin-module-resolver": "^5.0.0"
  },
  "private": true
} 